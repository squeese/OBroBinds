<Ui xmlns='http://www.blizzard.com/wow/ui/'>
  <Script file="root.lua" />

  <Frame name="OBroBindsRootTemplate" mixin="OBroBindsRootMixin" hidden="true" virtual="true" movable="true">
    <Size>
      <AbsDimension x="320" y="240" />
    </Size>
    <Anchors>
      <Anchor point="CENTER" />
    </Anchors>
    <Scripts>
      <OnLoad method="OnLoad" />
      <OnEvent method="OnEvent" />
      <OnShow method="OnShow" />
      <OnHide method="OnHide" />
    </Scripts>
  </Frame>

  <Frame name="OBroBindsPanelTemplate" mixin="OBroBindsPanelMixin" inherits="UIPanelDialogTemplate" virtual="true" enableMouse="true">
    <Anchors>
      <Anchor point="TOPLEFT" />
      <Anchor point="BOTTOMRIGHT" />
    </Anchors>
    <Scripts>
      <OnDragStart method="OnDragStart" />
      <OnDragStop method="OnDragStop" />
    </Scripts>
  </Frame>

  <Frame name="OBroBindsPageTemplate" mixin="OBroBindsPanelMixin" hidden="true" virtual="true">
    <Anchors>
      <Anchor point="TOPLEFT" />
      <Anchor point="BOTTOMRIGHT" />
    </Anchors>
    <Scripts>
      <OnShow method="OnShow" />
      <OnHide method="OnHide" />
    </Scripts>
  </Frame>
</Ui>
